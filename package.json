{
  "name": "@rxts/types",
  "version": "0.0.0",
  "description": "TypeScript Type Definitions same as DefinitelyTyped.",
  "repository": "git@github.com:rx-ts/types.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "run-s build:ts build:r",
    "build:r": "r -p",
    "build:ts": "tsc -b",
    "clean": "rimraf packages/*/lib",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,mdx,ts,tsx -f friendly",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "yarn-deduplicate && t || exit 0",
    "test": "ts-node scripts/dtslint",
    "type-coverage": "type-coverage --cache --detail --ignore-catch --ignore-files '**/*.d.ts' --strict -u"
  },
  "devDependencies": {
    "@1stg/lib-config": "^1.0.4",
    "@1stg/tslint-config": "^1.2.0",
    "@types/node": "^14.14.37",
    "dtslint": "^4.0.7",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.34.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "type-coverage": "^2.15.1",
    "typescript": "^4.1.5",
    "vue": "^3.0.5",
    "webpack": "^5.23.0",
    "yarn-deduplicate": "^3.1.0"
  },
  "resolutions": {
    "@babel/core": "^7.13.14",
    "prettier": "^2.2.1"
  },
  "commitlint": {
    "extends": [
      "@1stg"
    ]
  },
  "prettier": "@1stg/prettier-config",
  "renovate": {
    "extends": [
      "@1stg"
    ]
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
